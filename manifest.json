{
  "manifest_version": 3,
  "name": "Dropdown Extractor",
  "version": "1.0",
  "description": "Click any dropdown option to extract its list",

  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite",
    "storage"
  ],

  "action": {
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "background": {
    "service_worker": "background.js"
  }
}
