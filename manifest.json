{
  "manifest_version": 3,
  "name": "Dropdown Extractor",
  "version": "1.0.1",
  "description": "Click any dropdown option to extract its list",
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  }
}
